{
  "redirects": [
    {
      "source": "/privacy-policy",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/terms-of-service",
      "destination": "/", 
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/index.html"
    },
    {
      "source": "/terms-of-service",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 