{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "AZ Dining Saku/**",
      "use": "@vercel/static"
    },
    {
      "src": "logo/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/beta",
      "destination": "/beta-promo-system.html"
    },
    {
      "source": "/beta/(.*)",
      "destination": "/beta-promo-system.html"
    },
    {
      "source": "/marketing",
      "destination": "/marketing-website.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}